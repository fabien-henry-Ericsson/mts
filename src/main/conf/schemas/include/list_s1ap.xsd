<!-- liste des methodes pour la pile S1AP -->

<xs:element name="openChannelS1AP"   type="openChannelS1APType"   minOccurs="0" maxOccurs="unbounded">
    <xs:annotation>
        <xs:appinfo>mixed [0,N]</xs:appinfo>
        <xs:documentation>To open a channel for the S1AP protocol.</xs:documentation>
    </xs:annotation>
</xs:element>

<xs:element name="closeChannelS1AP"  type="closeChannelS1APType"  minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To close a channel for the S1AP protocol.</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="createListenpointS1AP"  type="createListenpointS1APType"  minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To start listening for the S1AP protocol.</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="removeListenpointS1AP"  type="removeListenpointS1APType"  minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To stop listening for the S1AP protocol.</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="sendMessageS1AP"      type="sendMessageS1APType"      minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To send a message for the S1AP protocol using a channel or with the transactionId (user defined).</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="receiveMessageS1AP"   type="receiveMessageS1APType"   minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To wait until a message of the S1AP protocol is received. The operation fails if no message is received until a given timeout value.</xs:documentation>
</xs:annotation>
</xs:element>
