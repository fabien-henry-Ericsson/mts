<!-- liste des methodes pour la pile NGAP -->

<xs:element name="openChannelNGAP"   type="openChannelNGAPType"   minOccurs="0" maxOccurs="unbounded">
    <xs:annotation>
        <xs:appinfo>mixed [0,N]</xs:appinfo>
        <xs:documentation>To open a channel for the NGAP protocol.</xs:documentation>
    </xs:annotation>
</xs:element>

<xs:element name="closeChannelNGAP"  type="closeChannelNGAPType"  minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To close a channel for the NGAP protocol.</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="createListenpointNGAP"  type="createListenpointNGAPType"  minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To start listening for the NGAP protocol.</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="removeListenpointNGAP"  type="removeListenpointNGAPType"  minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To stop listening for the NGAP protocol.</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="sendMessageNGAP"      type="sendMessageNGAPType"      minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To send a message for the NGAP protocol using a channel or with the transactionId (user defined).</xs:documentation>
</xs:annotation>
</xs:element>

<xs:element name="receiveMessageNGAP"   type="receiveMessageNGAPType"   minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
    <xs:appinfo>mixed [0,N]</xs:appinfo>
    <xs:documentation>To wait until a message of the NGAP protocol is received. The operation fails if no message is received until a given timeout value.</xs:documentation>
</xs:annotation>
</xs:element>
